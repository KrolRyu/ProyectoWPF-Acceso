<Window x:Class="ProyectoWPF_Acceso.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoWPF_Acceso"
        mc:Ignorable="d"
        Title="Smart Parking" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Bottom" x:Name="boton" Background="Transparent" BorderBrush="Transparent" Width="150" Height="150" Margin="10" Click="Button_Click">
                <Image Source="img/enter.png"></Image>
            </Button>
            <ContentControl x:Name="contentControl" Height="Auto" Width="Auto" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        </DockPanel>
    </Grid>
</Window>
